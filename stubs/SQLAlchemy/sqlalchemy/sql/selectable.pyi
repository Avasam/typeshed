        self,
        _label_style=...,
        use_labels: bool = ...,
        limit: int | None = ...,
        offset: int | None = ...,
        order_by: int | str | visitors.Traversible | Iterable[int | str | visitors.Traversible] | None = ...,
        group_by: int | str | visitors.Traversible | Iterable[int | str | visitors.Traversible] | None = ...,
        bind: Engine | Connection | None = ...,
    ) -> None: ...
    def with_for_update(
        self: Self,
        nowait: bool = ...,
        read: bool = ...,
        of: _ExpectElement | None = ...,
        skip_locked: bool = ...,
        key_share: bool = ...,
    ) -> Self: ...
    def get_label_style(self): ...
    def set_label_style(self: Self, style) -> Self: ...
    def apply_labels(self: Self) -> Self: ...
    def limit(self: Self, limit: int | str | visitors.Traversible | None) -> Self: ...
    def fetch(self: Self, count: int | str | visitors.Traversible | None, with_ties: bool = ..., percent: bool = ...) -> Self: ...
    def offset(self: Self, offset: int | str | visitors.Traversible | None) -> Self: ...
    def slice(self: Self, start: Incomplete | None, stop: Incomplete | None) -> Self: ...
    def order_by(self: Self, *clauses: _ExpectElement | None) -> Self: ...
    def group_by(self: Self, *clauses: _ExpectElement | None) -> Self: ...

