class FromClause(roles.AnonymizedFromClauseRole, Selectable):
    __visit_name__: str
    named_with_column: bool
    schema: str | None
    is_selectable: bool
    def select(self, whereclause: bool | str | visitors.Traversible | None = ..., **kwargs) -> Select: ...
    def join(self, right: FromClause, onclause: ClauseElement | None = ..., isouter: bool = ..., full: bool = ...) -> Join: ...
    def outerjoin(self, right: FromClause, onclause: ClauseElement | None = ..., full: bool = ...) -> Join: ...
    def alias(self, name: str | None = ..., flat: bool = ...) -> Alias: ...
    def table_valued(self) -> TableValuedColumn: ...
    def tablesample(self, sampling: float | Function, name: str | None = ..., seed: Incomplete | None = ...) -> TableSample: ...
    def is_derived_from(self, fromclause: FromClause) -> bool: ...
    @property
    def description(self) -> str: ...
    @property
    def exported_columns(self): ...
    @memoized_property
    def columns(self) -> ImmutableColumnCollection: ...
    @property
    def entity_namespace(self): ...
    @memoized_property
    def primary_key(self) -> ColumnSet: ...
    @memoized_property
    def foreign_keys(self) -> set[ForeignKey]: ...
    @property
    def c(self) -> ImmutableColumnCollection: ...

LABEL_STYLE_NONE: Any
LABEL_STYLE_TABLENAME_PLUS_COL: Any
LABEL_STYLE_DISAMBIGUATE_ONLY: Any
